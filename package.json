    "katex": "^0.16.11",
    "markdown-it": "https://github.com/dimaslanjaka/markdown-it/raw/95599a5/release/markdown-it.tgz",
    "sbg-utility": "^1.1.8",
    "upath": "^2.0.1"
  },
  "devDependencies": {
