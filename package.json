{
  "name": "markdown-it-mathematics",
  "version": "1.0.3",
  "description": "markdown-it mathematics plugin",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "default": "./dist/index.cjs",
        "types": "./dist/index.d.ts"
      },
      "import": {
        "default": "./dist/index.mjs",
        "types": "./dist/index.d.ts"
      }
    }
  },
  "directories": {
    "test": "test",
    "lib": "dist"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "husky",
    "build": "rimraf dist && rollup -c rollup.config.js && rollup -c rollup-all.config.js && tsc -b tsconfig.docs.json",
    "pack": "node packer.cjs --yarn"
  },
  "keywords": [
    "markdown",
    "KaTeX",
    "math",
    "LaTeX",
    "markdown-it-plugin",
    "markdown-it"
  ],
  "author": {
    "email": "dimaslanjaka@gmail.com",
    "name": "dimaslanjaka",
    "url": "https://webmanajemen.com"
  },
  "license": "MIT",
